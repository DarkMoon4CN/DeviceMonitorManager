@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>添加元素分类</title>
</head>
<body>
    <div style="padding: 15px; overflow: hidden">
        <form id="ItemTypeAddForm" method="post" action="">
            <table class="tableForm" width="100%">
                <tr>
                    <th>
                        元素分类名称：
                    </th>
                    <td>
                        <input type="text" name="txtItemType1" id="txtItemType1" class="easyui-validatebox textbox"
                               data-options="required:true" style="width:170px;height:22px;" />
                        <span style="color:gray;">必填</span>
                    </td>
                </tr>

                <tr>
                    <th>
                        排序：
                    </th>
                    <td>
                        <input name="txtItemTypeIndex1" id="txtItemTypeIndex1" value="1" class="easyui-numberspinner"
                               data-options="required:true,min:1,max:1000,editable:true" autocomplete="off" style="width:70px;height:22px;" />

                    </td>
                </tr>

            </table>
        </form>
    </div>

</body>
</html>
